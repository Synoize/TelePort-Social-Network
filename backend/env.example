# Server Port
PORT=5000

# For MongoDB Atlas (cloud):
MONGODB_URI=mongodb+srv://<USERNAME>:<PASSWORD>@cluster0.xxxxx.mongodb.net/<DATABASE_NAME>?retryWrites=true&w=majority&appName=<APP_NAME>

# JWT Secret Key
JWT_SECRET=jwt_secret_key
JWT_SECRET_EXPIRE=jwt_secret_expire

# Environment
NODE_ENV=development

